{
  "plugins": [
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-unused-vars" : "warn",
    "no-empty-pattern": "off",
    "prettier/prettier": "warn"
  },
  "globals": {
    "eslint" : true,
    "module" : true,
    "require" : true,
    "gapi" : true
  }
}
